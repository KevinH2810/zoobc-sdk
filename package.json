{
  "name": "zoobc-sdk-js",
  "version": "0.1.0",
  "description": "A small set of libraries to implement applications connect with the P2P API of the nodes in the blockchain",
  "author": "ZooBC SDK Team",
  "license": "Apache",
  "private": true,
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "keywords": [
    "blockchain",
    "blockchain-zoo",
    "zoobc",
    "zoobc-sdk-js"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/zoobc/zoobc-sdk-js"
  },
  "bugs": {
    "url": "https://github.com/zoobc/zoobc-sdk-js/issues"
  },
  "scripts": {
    "test": "jest",
    "testCoverage": "rm -rf coverage && jest --coverage",
    "build": "rm -rf dist && tsc -p .",
    "lint": "eslint \"{examples,src,tests}/**/*\" --fix",
    "prettier": "prettier --write \"{examples,src,tests}/**/*\"",
    "gh-pages": "rimraf docs && npm run docs && gh-pages -d docs",
    "docs": "typedoc --out docs src"
  },
  "dependencies": {
    "@grpc/proto-loader": "^0.5.1",
    "grpc": "^1.22.2",
    "protobufjs": "^6.8.8",
    "ts-protoc-gen": "^0.10.0"
  },
  "devDependencies": {
    "@types/jest": "^24.0.15",
    "@typescript-eslint/eslint-plugin": "^1.13.0",
    "@typescript-eslint/parser": "^1.13.0",
    "eslint": "^6.1.0",
    "eslint-config-prettier": "^6.0.0",
    "eslint-plugin-prettier": "^3.1.0",
    "gh-pages": "^2.0.1",
    "husky": "^3.0.1",
    "jest": "^24.8.0",
    "lint-staged": "^9.2.0",
    "prettier": "^1.18.2",
    "ts-jest": "^24.0.2",
    "typedoc": "^0.14.2",
    "typescript": "^3.5.3"
  }
}
